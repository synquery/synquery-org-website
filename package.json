    "@mdx-js/react": "*",
    "@mdx-js/mdx": "^1.6.5",
    "gatsby": "^2.0.0",
    "gatsby-plugin-intl": "^0.3.3",
    "axios": "^0.21.1",
    "clipboard": "^2.0.6",
    "cross-fetch": "^3.1.3-alpha.6",
    "dotenv": "^8.2.0",
    "framer-motion": "^4.1.3",
    "parcel-plugin-mdx": "@mdx-js/parcel-plugin-mdx",
    "lodash": "^4.17.21",
    "luxon": "^1.24.1",
    "netlify-lambda": "^2.0.3",
    "polished": "^4.1.1",
    "prismjs": "^1.25.0",
    "styled-components": "^5.1.1",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "parcel": "^2.0.1",
    "chokidar": "^3.5.2",
    "front-matter": "^4.0.2",
    "github-slugger": "^1.3.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "serve": "^13.0.2",
    "websockets": "synquery/node-websockets"
  },
  "resolutions": {
    "sharp": "0.27.0"
  },
  "scripts": {
    "build": "yarn merge-translations && yarn copy-contributors && yarn build:parcel",
    "build:lambda": "netlify-lambda build src/lambda",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "copy-contributors": "node src/scripts/copy-contributors.js",
    "generate-heading-ids": "node src/scripts/generate-heading-ids.js",
    "merge-translations": "node src/scripts/merge-translations.js",
    "start": "parcel serve ./src/index.html",
    "start-org": "yarn merge-translations && yarn copy-contributors",
    "start:lambda": "netlify-lambda serve src/lambda",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
